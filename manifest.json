{
  "manifest_version": 2,

  "name": "VUid",
  "description": "Gettin' uid or vid from Youku.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "static/power.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches":["http://*/*"],
      "js": ["/static/jquery.min.js","/static/base64.js","/static/popup.js"]
    }
  ],
  "background":{
        "persistent": false,
        "scripts":["/static/background.js"]
    },
  "permissions": [ "tabs","activeTab" ]
}
